/* pages/help/service/index.wxss */
page {
    background-color: #f5f5f5;
  }
  
.info{
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    padding-top: 130rpx;
}
.service {
    margin-top: 30rpx;
    width:670rpx;
    height:220rpx;
    background:rgba(255,255,255,1);
    box-shadow:0rpx 6rpx 20rpx 2rpx rgba(0,85,205,0.1);
    border-radius:14rpx;
    display: flex;
    align-items: center;
    padding-left: 40rpx;
}
.title{
    margin-left: 20rpx;
    font-size:38rpx;
    font-family:"PingFangSC-Medium,PingFang SC";
    font-weight:500;
    color:rgba(51,51,51,1);
}
.title button {
    background:rgba(255,255,255,1);
    font-size:38rpx;
    padding: 0rpx;
    margin: 0rpx;
}
.title button::after {
    border:none;
}