<!-- pages/status/index.wxml -->
<view class="page">
  <view class="status">
    <view class="status_msg">
      <van-icon color="#1759EF" name="passed" />
      <text>打卡成功</text>
    </view>
    <view class="status_text">您已经提交成功，可立即去查看打卡记录</view>
  </view>
  <view class="btn-item">
    <van-button bind:click="navigateTo" data-path='/pages/blockRecords/index' round="{{true}}" type="primary" color="#1759EF" block>
      打卡记录
    </van-button>
  </view>
  <view class="btn-item">
    <van-button bind:click="navigateTo" data-path='/pages/index/index' custom-style="" round="{{true}}" type="primary" color="#dddddd" block>
      <text class="btn_go-home">返回首页</text>
    </van-button>
  </view>
</view>