<!-- pages/status/index.wxml -->
<view class="page">
  <view class="status">
    <view class="status_icon">
      <image src="../../static/images/success@3x.png" />
    </view>
    <view class="status_msg">
      <text style='font-size:40rpx;'>{{msg}}</text>
    </view>
  </view>
  <view class="btn-item">
    <navigator url="/pages/index/index">
      <button bind:click="navigateTo" style="font-size:35rpx;width:300rpx;background-color:#1759ef;color:#fff;"   block>返回首页</button>
    </navigator>
  </view>
