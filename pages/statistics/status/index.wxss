/* pages/statistics/status/index.wxss */
page {
    box-sizing: border-box;
    height: 100%;
    padding: 0 30rpx 0;
    background: #f5f5f5; }
  
  .status {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center; }
  
  .status_icon {
    margin-top: 120rpx; }
    .status_icon image {
      width: 200rpx;
      height: 200rpx; }
  
  .status_msg {
      margin-top: 20rpx;
    align-items: center;
    font-size: 30rpx;
    color: #000; }
  
  .btn-item {
    /* margin: 200rpx auto 0; */
    text-align: center;
    display: flex;
    justify-content: center;
    align-items: center;
    width: 300rpx;
    height: 90rpx;
    border-radius: 10rpx;
    background-color: #1759ef; }
  .title {
      margin-top: 80rpx;
      padding: 0 165rpx;
  }
  .one {
      display: flex;
      justify-content: flex-start;
      align-items: center;
      margin-bottom: 20rpx;
  }
  .number {
    background:rgba(23,89,239,1);
      width: 32rpx;
      height: 32rpx;
    border-radius: 50%;
    text-align: center;
    font-size:24rpx;
    font-family:PingFangSC-Regular,PingFang SC;
    font-weight:400;
    color:rgba(255,255,255,1);
    line-height:32rpx;
  }
  .text {
      margin-left: 25rpx;
    font-size:24rpx;
    font-family:PingFangSC-Regular,PingFang SC;
    font-weight:400;
    color:rgba(51,51,51,1);
  }
    .daochu {
        width: 330rpx;
        height: 197rpx;
    }
  

  .btn {
      margin-top: 300rpx;
      padding: 0 35rpx;
      display: flex;
      justify-content: space-between;

  }

.t .van-button--primary {
    color: #333333 !important;
}    