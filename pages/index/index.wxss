page {
  box-sizing: border-box;
  position: relative;
  min-height: 100%;
  padding-bottom: 150rpx;
  background-color: #f5f5f5;
  font-family: PingFangSC-Regular;
}

.footer-copy-right {
  box-sizing: border-box;
  position: absolute;
  bottom: 0;
  display: block;
  width: 100%;
  height: 100rpx;
  font-size: 30rpx;
  color: #acabab;
  line-height: 100rpx;
  text-align: center;
  background-color: #f5f5f5;
}

