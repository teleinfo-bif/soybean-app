.logo{
  width: 100%;
  text-align:center;
  margin-top:30rpx;
  position:absolute;
  bottom:40rpx;
}
.copyright-view{
  margin-top: -20rpx;
  position: fixed;
  text-align: center;
  width: 100%;
  height: 72rpx;
  line-height: 50rpx;
  font-size: 28rpx;
  bottom:0px;
  color: #acabab;
  background-color: #f5f5f5;
  z-index: -1;
}

.homelogo {
  display: inline-block;
  margin-top: 15rpx;
  width: 250rpx;
  height: 30rpx;
}

.page__title{
 font-size: 28rpx;
 line-height: 30rpx;
 color: #acabab;
}


.help {
  margin-top: 13rpx;
  display: flex;
  align-items: center;
  justify-content: flex-end;
}
.help-img {
  display: block;
  width: 30rpx;
  height: 30rpx;
}
.help-text {
  display: block;
  margin-left: 11rpx;
  margin-right: 60rpx;
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(153,153,153,1);
  line-height:24rpx;
}