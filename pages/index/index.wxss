page {
  box-sizing: border-box;
  position: relative;
  min-height: 100%;
  padding-bottom: 65rpx;
  background-color: #f5f5f5;
  font-family: PingFangSC-Regular;
}

.footer-copy-right {
  box-sizing: border-box;
  position: absolute;
  bottom: 0;
  display: block;
  width: 100%;
  height: 100rpx;
  font-size: 30rpx;
  color: #acabab;
  line-height: 100rpx;
  text-align: center;
  background-color: #f5f5f5;
}

.logo{
  width: 100%;
  position: fixed;
  bottom:0px;
  display: flex;
  justify-content: center;
}
 
.copyright-view{
  margin-top: -20rpx;
  position: fixed;
  text-align: center;
  width: 100%;
  height: 72rpx;
  line-height: 50rpx;
  font-size: 28rpx;
  bottom:0px;
  color: #acabab;
  background-color: #f5f5f5;
  z-index: -1;
}
