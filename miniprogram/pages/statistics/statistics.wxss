.dashboard {
  width: 720rpx;
  height: 250rpx;
  margin: auto;
  margin-top: 15rpx;
  border-radius: 6rpx;
  background-color: #ffffff;
  box-shadow:0 6rpx 28rpx 0rpx rgba(194, 199, 225, 0.47);
